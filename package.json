    "coinexo:env": "cp -a whitelabels/coinexo/environments src/",
    "coinexo:android:dev": "npm run coinexo:env && cp -a whitelabels/coinexo/resources whitelabels/coinexo/development/config.xml . && ionic cordova run android --no-native-run",
    "coinexo:android:livereload": "npm run coinexo:env &&  cp -a whitelabels/coinexo/resources whitelabels/coinexo/development/config.xml . && ionic cordova run android -l --address=0.0.0.0 --no-native-run",
    "coinexo:android:prod": "npm run coinexo:env && cp -a whitelabels/coinexo/resources whitelabels/coinexo/production/config.xml . && ionic cordova run android --no-native-run --prod",
    "coinexo:android:release": "npm run coinexo:env && cp -a whitelabels/coinexo/resources whitelabels/coinexo/production/config.xml . && ionic cordova build android --prod --release",
    "coinexo:ios:dev": "npm run coinexo:env && cp -a whitelabels/coinexo/resources whitelabels/coinexo/development/config.xml . && ionic cordova run ios",
    "coinexo:ios:livereload": "npm run coinexo:env &&  cp -a whitelabels/coinexo/resources whitelabels/coinexo/development/config.xml . && ionic cordova run ios -l",
    "coinexo:ios:prod": "npm run coinexo:env && cp -a whitelabels/coinexo/resources whitelabels/coinexo/production/config.xml . && ionic cordova run ios --prod"
  },
